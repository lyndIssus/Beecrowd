using System;
class URI{
    static void Main(string[] args){
        int[] cedulas = new int[7] {100,50,20, 10, 5, 2, 1};
        int A = int.Parse(Console.ReadLine());
        Console.WriteLine(A);
        for (int i = 0; i<cedulas.Length;i++){
            Console.WriteLine(A/cedulas[i] + " nota(s) de R$ "+ cedulas[i] + ",00");
            A = A%cedulas[i];

        }
        

        
        
        
}
}
