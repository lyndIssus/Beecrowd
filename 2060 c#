using System;
using System.Globalization;

class URI{
    static void Main(string[] args){
    string CorAnswer = Console.ReadLine();
    string[] Answers = Console.ReadLine().Split(' ');
    int counter = 0;
    foreach(string s in Answers){
        if(s == CorAnswer){
            counter+=1;
        }
    }
    Console.WriteLine(counter);
    
    }}  
